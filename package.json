{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=production netlify dev",
    "build": "npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/main.css -o src/styles/main.css",
    "dev": "concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/main.css -o src/styles/main.css",
    "test": "vitest"
  },
  "dependencies": {
    "@netlify/functions": "^1.0.0",
    "@remix-run/netlify": "^1.6.8",
    "@remix-run/node": "^1.6.8",
    "@remix-run/react": "^1.6.8",
    "@supabase/auth-helpers-shared": "^0.1.3",
    "@supabase/supabase-js": "^2.0.0-rc.3",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.2",
    "date-fns-tz": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.8",
    "@remix-run/eslint-config": "^1.6.8",
    "@remix-run/serve": "^1.6.8",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "eslint": "^8.20.0",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "vitest": "^0.22.1"
  },
  "engines": {
    "node": ">=14"
  }
}
